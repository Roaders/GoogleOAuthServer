{
  "name": "youtubeoauth",
  "version": "1.0.12",
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.8"
  },
  "description": "OAuth Server for authenticating users to access the youTube api",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "start": "node index.js",
    "launch": "npm run compile && npm start",
    "typings": "typings install",
    "postinstall": "npm run clean && npm run typings && npm run compile",
    "clean": "rimraf *.js",
    "push:git": "git push --tags origin master",
    "push:heroku": "git push --tags heroku master",
    "version:patch": "npm version patch",
    "postversion:patch": "npm run push:git",
    "deploy": "npm run clean && npm run compile && npm run version:patch && npm run push:heroku"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Roaders/YouTubeOAuth.git"
  },
  "keywords": [
    "node",
    "youtube",
    "oauth"
  ],
  "author": "Giles Roadnight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Roaders/YouTubeOAuth/issues"
  },
  "homepage": "https://github.com/Roaders/YouTubeOAuth#readme",
  "dependencies": {
    "express": "^4.14.0",
    "node-env-file": "^0.1.8",
    "rimraf": "^2.5.4",
    "rx": "^4.1.0",
    "rx-node": "^1.0.2",
    "typescript": "1.8.10",
    "typings": "^2.0.0"
  }
}
